{
  "name": "@hexagon6/sapper-cellular-automata",
  "description": "Cellular Automata built with svelte app maker",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "export": "sapper export",
    "start": "sapper start",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "unit": "ava",
    "unit:watch": "ava --watch",
    "test:dev": "run-p --race dev unit",
    "test": "run-s build unit"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "node-fetch": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "sapper": "^0.8.1",
    "serve-static": "^1.13.1",
    "svelte": "^1.56.0",
    "svelte-loader": "^2.3.3",
    "webpack": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "@std/esm": "^0.25.2",
    "ava": "1.0.0-beta.3"
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "sources": [
      "modules/*.{js}"
    ],
    "require": [
      "@std/esm",
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "plugins": [],
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "@std/esm": "js"
}
